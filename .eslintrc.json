{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react-hooks/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
		
}
